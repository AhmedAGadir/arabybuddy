{
  "name": "arabybuddy",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "EXPO_UNSTABLE_MCP_SERVER=1 expo start --dev-client",
    "dev:local": "EXPO_UNSTABLE_MCP_SERVER=1 REACT_NATIVE_PACKAGER_HOSTNAME=localhost expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "eas:whoami": "eas whoami",
    "build:list": "eas build:list",
    "build:cancel": "eas build:cancel",
    "build:ios:development": "eas build --platform ios --profile development",
    "build:android:development": "eas build --platform android --profile development",
    "build:ios:production": "eas build --platform ios --profile production",
    "build:android:production": "eas build --platform android --profile production",
    "install:ios": "eas build:run --platform ios",
    "install:android": "eas build:run --platform android",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "maestro test .maestro/",
    "database:pull": "npx supabase db pull",
    "database:types": "npx supabase gen types typescript --local > src/types/database.types.ts",
    "gluestack:mcp": "cd ../gluestack-mcp && node index.js"
  },
